# Cloudflare Analytics 配置示例
# 复制此文件为 .env 并填入你的实际配置

# 方式1: 简单配置（单账户）
CF_TOKENS=your_cloudflare_api_token_here
CF_ZONES=zone_id_1,zone_id_2,zone_id_3
CF_DOMAINS=example.com,cdn.example.com,api.example.com
CF_ACCOUNT_NAME=我的主账户

# 方式2: 多账户配置（取消注释并使用）
# CF_TOKENS_1=token_for_account_1
# CF_ZONES_1=zone1,zone2
# CF_DOMAINS_1=site1.com,site2.com
# CF_ACCOUNT_NAME_1=账户1

# CF_TOKENS_2=token_for_account_2
# CF_ZONES_2=zone3,zone4
# CF_DOMAINS_2=site3.com,site4.com
# CF_ACCOUNT_NAME_2=账户2

# 方式3: JSON格式配置
# CF_CONFIG={"accounts":[{"name":"主账号","token":"your_token","zones":[{"zone_id":"zone1","domain":"example.com"},{"zone_id":"zone2","domain":"cdn.example.com"}]}]}

# 系统配置
PORT=4000
NGINX_PORT=80